%h1= t('project.new.heading')
- form_for @project, :html => {:multipart => true} do |f|
  %p
    = t('project.name')
    = f.text_field :name
  %p
    = t('project.description')
    = f.text_area :description
  %p
    = t('project.picture')
    = f.file_field :picture
  %p
    = t('project.logo')
    = f.file_field :logo
  %p
    = submit_tag t('project.new.submit')
    or
    = link_to 'Cancel', projects_path
  